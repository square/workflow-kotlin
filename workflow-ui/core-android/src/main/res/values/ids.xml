<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!--
  BackStackContainer restricts animation to child with this id, if there is one.
  Otherwise animates its entire body.
   -->
  <item name="back_stack_body" type="id"/>
  <!-- View Tag that holds the function to update a view created via ViewRegistry.buildView. -->
  <item name="legacy_workflow_view_state" type="id"/>
  <!-- View Tag used for back button handlers. -->
  <item name="view_back_handler" type="id"/>
  <!-- View Tag used for EditText textChangedListener. -->
  <item name="view_text_changed_listener" type="id"/>
  <!-- View Tag used by View.launchWhenAttached. -->
  <item name="view_attached_coroutine_scope" type="id"/>
  <!-- View Tag used by TextController. -->
  <item name="text_controller_rendering" type="id"/>
  <!-- View Tag holding the BackStackScreen.Key associated with a view. -->
  <item name="workflow_back_stack_key" type="id"/>
  <!-- ID used by BackStackContainer to opt in to view persistence. -->
  <item name="workflow_back_stack_container" type="id"/>
  <!-- ID used by BodyAndModalsContainer to opt in to view persistence. -->
  <item name="workflow_body_and_modals_container" type="id"/>
  <!-- Default id used by WorkflowLayout to opt in to view persistence. -->
  <item name="workflow_layout" type="id"/>
  <!-- View Tag for the ViewEnvironment that last updated this view. -->
  <item name="workflow_environment" type="id"/>
  <!-- View Tag for the Screen that last updated this view. -->
  <item name="workflow_screen" type="id"/>
  <!-- View Tag for the Overlay that last updated this Dialog, set on its decor view. -->
  <item name="workflow_overlay" type="id"/>
</resources>
